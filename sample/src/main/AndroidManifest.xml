<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.github.anzewei.sample">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".NormalActivity"
            android:configChanges="orientation"
            android:label="NormalActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".basic.BackExtendsBaseActivity"
            android:label="BackExtendsBaseActivity">
        </activity>
        <activity android:name=".basic.TopActivity"/>
        <activity android:name=".basic.LeftActivity"/>
        <activity android:name=".basic.BottomActivity"/>
        <activity android:name=".basic.RightActivity"/>
        <activity android:name=".basic.NoneActivity"/>
    </application>

</manifest>